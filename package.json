{
  "name": "jetstate",
  "version": "0.0.7",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "author": {
    "name": "Mikhail Nasyrov",
    "email": "mnasyrov.apps@gmail.com",
    "url": "https://mnasyrov.com"
  },
  "homepage": "https://github.com/mnasyrov/jetstate#readme",
  "bugs": {
    "url": "https://github.com/mnasyrov/jetstate/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mnasyrov/jetstate.git"
  },
  "keywords": [
    "state-management",
    "state",
    "angular",
    "rxjs",
    "ngx",
    "angular2",
    "cqrs",
    "store"
  ],
  "devDependencies": {
    "@angular/compiler": "7.1.4",
    "@angular/compiler-cli": "7.1.4",
    "@angular/core": "7.1.4",
    "ng-packagr": "4.4.5",
    "prettier": "1.15.3",
    "rxjs": "6.3.3",
    "tsickle": "0.34.0",
    "tslib": "1.9.3",
    "tslint": "5.12.0",
    "tslint-config-prettier": "1.17.0",
    "typescript": "3.1.6",
    "wsrun": "3.5.0",
    "zone.js": "0.8.26"
  },
  "scripts": {
    "clean": "wsrun clean",
    "lint": "tslint \"packages/*/src/**/*.ts\"",
    "format-check": "prettier --list-different \"packages/*/{src,test,itest}/**/*.ts\"",
    "format": "prettier --write \"packages/*/{src,test,itest}/**/*.ts\"",
    "build": "wsrun --stages build",
    "itest": "wsrun --stages --exclude-missing itest",
    "commit-check": "yarn lint && yarn format-check && yarn build && yarn itest",
    "publish-check": "wsrun --serial --bin npm publish --dry-run dist",
    "publish-do": "wsrun --serial --bin npm -- publish dist --access public"
  }
}
