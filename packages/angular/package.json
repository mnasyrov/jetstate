{
  "name": "@jetstate/angular",
  "version": "0.0.20",
  "license": "MIT",
  "author": {
    "name": "Mikhail Nasyrov",
    "email": "mnasyrov.apps@gmail.com",
    "url": "https://mnasyrov.com"
  },
  "homepage": "https://github.com/mnasyrov/jetstate#readme",
  "bugs": {
    "url": "https://github.com/mnasyrov/jetstate/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mnasyrov/jetstate.git"
  },
  "keywords": [
    "state-management",
    "store",
    "state",
    "cqrs",
    "rxjs",
    "angular",
    "angular2",
    "ngx"
  ],
  "peerDependencies": {
    "@angular/core": ">=5.0.0"
  },
  "dependencies": {
    "@jetstate/rx": "0.0.20",
    "tslib": "^1.9.0"
  },
  "sideEffects": false,
  "ngPackage": {
    "lib": {
      "entryFile": "index.ts",
      "umdModuleIds": {
        "@jetstate/core": "jetstate.core",
        "@jetstate/rx": "jetstate.rx"
      }
    },
    "dest": "../../build/@jetstate/angular",
    "whitelistedNonPeerDependencies": [
      "@jetstate/rx"
    ]
  },
  "scripts": {
    "build": "ng-packagr -p package.json",
    "version": "npe 'dependencies.@jetstate/rx' ${npm_package_version}"
  }
}
