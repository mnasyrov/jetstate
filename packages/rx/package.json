  },
  "keywords": [
    "state-management",
    "store",
    "state",
    "cqrs",
    "rxjs"
  ],
  "peerDependencies": {
    "rxjs": ">=6.0.0 || ^5.6.0-forward-compat.4"
  },
  "dependencies": {
    "@jetstate/core": "0.0.19",
    "tslib": "^1.9.0"
  },
  "sideEffects": false,
  "ngPackage": {
    "lib": {
      "entryFile": "index.ts",
      "umdModuleIds": {
        "@jetstate/core": "jetstate.core"
      }
    },
    "dest": "../../build/@jetstate/rx",
    "whitelistedNonPeerDependencies": [
      "@jetstate/core"
    ]
  },
  "scripts": {
    "build": "ng-packagr -p package.json",
    "version": "npe 'dependencies.@jetstate/core' ${npm_package_version}"
  }
